A. Dima and Friends

 
import java.util.*;
import java.lang.*;

 public class c{

static void print(String s)
{
    System.out.print(s);
}
public static void main(String[] args)
{
    Scanner sc = new Scanner(System.in);

    int n = sc.nextInt();

    int sum = 0;

  for(int i=0;i<n;i++)
    sum = sum + sc.nextInt();

    

    int count = 0;
    for(int i=1;i<=5;i++)
    {
        if((sum+i)%(n+1) != 1 )
            count++;
    }

    System.out.println(count);

	 
}

}
