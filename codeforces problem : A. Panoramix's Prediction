A. Panoramix's Prediction

// A Java program to demonstrate working of exit()
import java.util.*;
import java.lang.*;

 public class c{

public static void main(String[] args)
{
	Scanner sc = new Scanner(System.in);

    int  n , m;

    n = sc.nextInt();
    m = sc.nextInt();


    while(n+1<=m)
    {
    	int check = n+1;
    	boolean enter = true;
    	for(int i=2;i*i<=check;i++)
    	{
    		if(check%i==0)
    		{
    			enter = false;
    			 break;
    		}
    	}

    	if(enter )
    	{
    		 if(check == m )
	System.out.println("YES");
       else 
       	System.out.println("NO");

       System.exit(0);
    	 
    	}
    	
    	n++;

    }

    	System.out.println("NO");
 

}

}
