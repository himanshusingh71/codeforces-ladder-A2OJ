A. Perfect Permutation

 
import java.util.*;
import java.lang.*;

 public class c{

static void print(String s)
{
    System.out.print(s);
}
public static void main(String[] args)
{
	Scanner sc = new Scanner(System.in);
    
    int n = sc.nextInt();

    if(n%2!=0)
    {
        print(-1 + "");
    }
    else{
        int i=1;
        while(i<n)
        {
            System.out.print(i+1 + " " + i + " ");
            i+=2;
        }
    }
 
    print("");
}

}
